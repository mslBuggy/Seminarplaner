<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="bpHaupt" prefHeight="400.0" prefWidth="800.0" styleClass="hintergrund" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="referentenplanung.PlanungReferentController">
   <top>
      <FlowPane alignment="CENTER" hgap="10.0" prefHeight="50.0" prefWidth="800.0" styleClass="flowpane" BorderPane.alignment="CENTER">
         <children>
            <Label text="Referentenplanung" />
            <Label text="Referent" />
            <ComboBox fx:id="cbReferent" onAction="#cbReferent_Click" prefWidth="150.0" />
            <Label text="Jahr" />
            <ComboBox fx:id="cbJahr" onAction="#cbJahr_Click" prefHeight="25.0" prefWidth="80.0" styleClass="combo-box" />
            <Button fx:id="btDrucken" mnemonicParsing="false" onAction="#btDrucken_Click" text="Drucken" />
         </children>
      </FlowPane>
   </top>
  
</BorderPane>
