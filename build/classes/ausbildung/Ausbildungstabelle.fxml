<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="600.0" prefWidth="600.0" styleClass="hintergrund" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ausbildung.AusbildungstabelleController">
   <top>
      <FlowPane alignment="CENTER" prefHeight="76.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="TOP_LEFT" styleClass="schriftLabel" text="Ausbildungen" />
         </children></FlowPane>
   </top>
   <center>
      <Pane prefHeight="501.0" prefWidth="476.0" BorderPane.alignment="CENTER">
         <children>
            <TableView layoutX="21.0" layoutY="22.0" prefHeight="117.0" prefWidth="504.0">
              <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
              </columns>
            </TableView>
         </children>
      </Pane>
   </center>
   <bottom>
      <FlowPane alignment="CENTER_RIGHT" prefHeight="72.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="Neu">
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="btDetails" mnemonicParsing="false" onAction="#btDetails_Click" text="Details">
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
            <Button mnemonicParsing="false" text="Schliessen" />
         </children>
      </FlowPane>
   </bottom>
</BorderPane>
